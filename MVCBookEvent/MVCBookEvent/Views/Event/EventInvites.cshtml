@model List<BookEvent.Models.Event>

<h2>Event Invites</h2>

@if (Model.Count == 0)
{
    <p>You have not been invited to any events.</p>
}
else
{
    <ul>
        @foreach (var ev in Model)
        {
            <li><a href="@Url.Action("Details", "Event", new { id = ev.EventId })">@ev.EventName</a></li>
        }
    </ul>
}
